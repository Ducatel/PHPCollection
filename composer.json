{
    "name": "ducatel/PHPCollection",
    "type": "library",
    "description": "Implementation of data structure in PHP",
    "keywords": [
        "ducatel",
        "PHPCollection"
    ],
    "homepage": "https://github.com/Ducatel/PHPCollection",
    "license": "MIT",
    "authors": [
        {
            "name": "David Ducatel",
            "email": "david@ducatel.eu",
            "homepage": "http://ducatel.eu",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Ducatel\\PHPCollection\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ducatel\\PHPCollection\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
